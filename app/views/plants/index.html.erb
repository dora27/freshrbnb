<div class="navbar-marged">
<h2>style="text-align:center">Nature is everywhere</h2>
<div class="container-fluid">
  <div class="row">
    <div class="col-lg-7">
      <div class="row">
      <% @plants.each do |plant| %>
        <% if plant.photo %>
          <div class="col-xs-12 col-lg-6">
            <%= render 'shared/card', plant: plant %>
          </div>
        <% end %>
      <% end %>
     </div>
  </div>
  <div class="col-lg-5 no-padding">
    <%= render 'shared/googlemap' %>
  </div>
</div>
